{
	"id": "com.sap.sample.googlePieChart",
	"version": "1.0.0",
	"name": "Google Pie Chart",
	"description": "A Google Pie Chart",
	"newInstancePrefix": "googlePieChart",
	"icon": "https://ratishraj.github.io/GooglePieChart/PieChart.png",
	"vendor": "BGSW",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sap-sample-google",
			"url": "https://ratishraj.github.io/GooglePieChart/googlePieChart.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"Label": {
			"type": "string[]",
			"description": "label",
			"default": ["RED","green","blue"]
			
		},
		"Value": {
			"type": "integer[]",
			"description": "values",
			"default": [10, 20, 30]
			
		}
	},
	"methods": {

		"setLabel": {
			"description": "Sets the label.",
			"parameters": [
				{
					"name": "newLabel",
					"type": "string[]",
					"description": "The new label value."
				}
			],
			"body": "this.Label = newLabel;"
		},
		"getLabel": {
			"returnType": "string[]",
			"description": "Returns the label value",
			"body": "return this.Label;"
		},

		"setValue": {
			"description": "Sets the value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "integer[]",
					"description": "The new Value."
				}
			],
			"body": "this.Value = newValue;"
		},
		"getValue": {
			"returnType": "integer[]",
			"description": "Returns the value",
			"body": "return this.Value;"
		}	
	},
	"events": {
	}

}